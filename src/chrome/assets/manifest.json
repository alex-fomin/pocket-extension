{
  // Required
  "manifest_version": 2,
  "name": "Add to Pocket",
  "version": "3.0.0",
  "icons": {
    "128": "logo-128.png",
    "48": "logo-48.png"
  },
  "page_action": {
    "default_icon": {
      "19": "notAdded-19.png",
      "38": "notAdded-38.png"
    },
    "default_title": "Add item to Pocket"
  },
  "permissions": [
    "tabs",
    "management",
    "http://*/",
    "https://*/",
    "webNavigation"
  ]
}